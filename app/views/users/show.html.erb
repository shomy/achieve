<div class="container fluid">
      <h3><%= @user.name %></h3>
      <br><br>
<div class="row"><h3>フォローしている人</h3></div>
  <div class="row">
    <div class="col-md-8 col-md-offset-2">
      <% @follwed.each do |f| %>
         <%= f.name %>
         <%= profile_img(f) %>
      <% end %>
    </div>
  </div><br><br>
<div class="row"><h3>フォロワー</h3></div>
  <div class="row">
    <div class="col-md-8 col-md-offset-2">
       <% @followers.each do |f| %>
       <%= f.name %>
       <%= profile_img(f) %>
      <% end %>
    </div>
  </div>
</div>